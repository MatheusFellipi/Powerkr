{
  "compilerOptions": {
    "module": "ES2020",
    "jsx": "react-native",
    "esModuleInterop": true,
    "baseUrl": ".",
    "paths": {
      "@assets/*": ["assets/*"],
      "@services/*": ["src/services/*"],
      "@navigation/*": ["src/navigation/*"],
      "@components/*": ["src/components/*"],
      "@constant/*": ["src/shared/constant/*"],
      "@context/*": ["src/context/*"],
      "@shared/*": ["src/shared/*"],
      "@custom-types/*": ["src/types/*"]
    }
  },
  "extends": "expo/tsconfig.base"
}
