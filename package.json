    "@tamagui/card": "^1.88.18",
    "@tamagui/config": "^1.81.4",
    "@tamagui/font-inter": "^1.81.4",
    "@tamagui/form": "^1.88.19",
    "@tamagui/themes": "^1.81.4",
    "@tamagui/toast": "^1.88.19",
    "axios": "^1.6.7",
    "burnt": "^0.12.2",
    "date-fns": "^3.3.1",
    "expo": "49.0.21",
    "expo-constants": "~14.4.2",
    "expo-font": "11.4.0",
    "expo-linear-gradient": "~12.3.0",
    "expo-linking": "~5.0.2",
    "expo-router": "2.0.14",
    "expo-splash-screen": "~0.20.5",
    "expo-status-bar": "~1.6.0",
    "jest": "^29.2.1",
    "jest-expo": "~49.0.0",
    "moti": "^0.27.2",
    "react": "18.2.0",
